{
  "name": "@bullshit-prod/discord.db",
  "version": "0.0.3",
  "description": "Discord as a database",
  "author": "",
  "license": "MIT",
  "packageManager": "pnpm@10.24.0",
  "repository": {
  "type": "git",
  "url": "https://github.com/BullShitProd/DiscordDB.git"
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "discord",
    "database",
    "storage",
    "discorddb"
  ],
  "scripts": {
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "build": "tsdown",
    "test": "vitest run",
    "test:watch": "vitest",
    "example": "tsx examples/index.ts",
    "prepublishOnly": "pnpm run build",
    "prepare": "husky"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.2.0",
    "@types/node": "^24.10.1",
    "eslint": "^9.39.1",
    "husky": "^9.1.7",
    "tsdown": "^0.18.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^4.0.14"
  },
  "dependencies": {
    "discord.js": "^14.25.1"
  }
}
